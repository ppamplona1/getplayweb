.person-table{   
	border-collapse:collapse;
}

.person-table-header{
	text-align:center;
	background:none repeat scroll 0 0 steelblue;
	border-bottom:1px solid;
	padding:16px;
}

.person-table-odd-row{
	text-align:center;
        background:none repeat scroll 0 0 #66ccff;
	border-top:1px solid #BBBBBB;
}

.person-table-even-row{
	text-align:center;
	background:none repeat scroll 0 0 steelblue;
	border-top:1px solid #BBBBBB;
}
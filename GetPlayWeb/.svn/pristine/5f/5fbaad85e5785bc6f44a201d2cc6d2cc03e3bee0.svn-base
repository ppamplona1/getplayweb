package pt.uc.dei.aor.projeto3.grupod.managedBeans;

import java.io.Serializable;
import javax.annotation.security.PermitAll;
import javax.faces.bean.ManagedBean;
import javax.faces.bean.SessionScoped;
import pt.uc.dei.aor.projeto3.grupod.entities.UserPlay;

/**
 *
 * @author brunocosta
 */
@ManagedBean
@SessionScoped
@PermitAll
public class LogedUserManagedBean implements Serializable{

    private UserPlay userLoged;
    
    public LogedUserManagedBean() {
    }

    public UserPlay getUserLoged() {
        return userLoged;
    }

    public void setUserLoged(UserPlay userLoged) {
        this.userLoged = userLoged;
    }
    
}

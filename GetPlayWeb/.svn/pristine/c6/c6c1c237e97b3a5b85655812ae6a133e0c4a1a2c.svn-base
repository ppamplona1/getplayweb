 * @author User
 */
@ManagedBean
@RequestScoped
public class NavigationManagedBean {

    /**
     * Creates a new instance of NavigationManagedBean
     */
    public NavigationManagedBean() {
    }
    
    public String allMusics(){
        return "allMusics";
    }
    
    public String myMusics(){
        return "myMusics";
    }
    public String mainWindow(){

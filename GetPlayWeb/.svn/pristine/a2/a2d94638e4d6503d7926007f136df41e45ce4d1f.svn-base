<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html">
    <h:head>
        <title>Facelet Title</title>
    </h:head>
    <h:body>
        Hello from Facelets
        <h:form enctype="multipart/form-data">
        <h:outputLabel value="Titulo"></h:outputLabel>
        <h:inputText value="#{CreateMusicManagedBean.title}"></h:inputText>
        <br />
        <h:outputLabel value="Artista"></h:outputLabel>
        <h:inputText value="#{CreateMusicManagedBean.artist}"></h:inputText>
        <br />
        <h:outputLabel value="Album"></h:outputLabel>
        <h:inputText value="#{CreateMusicManagedBean.album}"></h:inputText>
        <br />
        <h:outputLabel value="Release Year"></h:outputLabel>
        <h:inputText value="#{CreateMusicManagedBean.releaseYEAR}"></h:inputText>
        <br />
        <h:outputLabel value="Ficheiro"></h:outputLabel>
        <h:inputFile value="#{CreateMusicManagedBean.path}"></h:inputFile>
        <br />
        <h:commandButton value="Save" action="#{CreateMusicManagedBean.newMusic}"></h:commandButton>
        </h:form>
    </h:body>
</html>

